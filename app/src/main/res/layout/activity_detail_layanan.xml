<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    android:id="@+id/row_container"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorbg"
    tools:context=".Screen.DetailLayanan">


    <LinearLayout
        android:layout_marginBottom="11dp"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <RelativeLayout
            android:layout_marginTop="-5dp"
            android:id="@+id/relativeLayout"
            android:layout_width="wrap_content"
            android:background="@drawable/icbg"
            android:layout_height="300dp"
            tools:ignore="MissingConstraints">

            <RelativeLayout
                android:layout_marginTop="5dp"
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                tools:ignore="MissingConstraints">

                <ImageView

                    android:id="@+id/kembali"
                    android:background="?attr/selectableItemBackgroundBorderless"
                    android:clickable="true"
                    android:layout_marginTop="20dp"
                    android:layout_marginLeft="20dp"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:src="@drawable/icback"></ImageView>
                <TextView
                    android:id="@+id/id"
                    android:layout_marginLeft="80dp"
                    android:layout_marginTop="23dp"
                    android:textColor="#ffffff"
                    android:textSize="25dp"
                    android:fontFamily="@font/pp"
                    android:text="@string/layanan"
                    android:visibility="invisible"
                    android:layout_width="match_parent"
                    android:layout_height="50dp" />
                <TextView
                    android:id="@+id/titlemenu"
                    android:layout_marginLeft="80dp"
                    android:layout_marginTop="23dp"
                    android:textColor="#ffffff"
                    android:textSize="25dp"
                    android:fontFamily="@font/pp"
                    android:text="@string/layanan"
                    android:layout_width="match_parent"
                    android:layout_height="50dp" />


            </RelativeLayout>
        </RelativeLayout>

        <androidx.core.widget.NestedScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            >

            <!--        <include-->
            <!--            android:id="@+id/edit"-->
            <!--            layout="@layout/dataactivity"-->
            <!--            tools:layout_editor_absoluteX="0dp"-->
            <!--            tools:layout_editor_absoluteY="0dp" />-->

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center">

                <TextView
                    android:id="@+id/judul"
                    android:layout_width="match_parent"
                    android:layout_height="43dp"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginRight="20dp"
                    android:layout_marginBottom="5dp"
                    android:fontFamily="@font/pp"
                    android:text="@string/brand"
                    android:textAlignment="center"
                    android:textColor="@color/colorBlack"
                    android:textSize="24sp" />


                <TextView
                    android:id="@+id/descbrand"
                    android:textAlignment="center"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginRight="20dp"
                    android:layout_marginBottom="5dp"
                    android:fontFamily="@font/pl"
                    android:textColor="@color/colorBlack"
                    android:text="@string/isiprofilsd2"
                    android:textSize="14sp">

                </TextView>

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">


                    <Button
                        android:id="@+id/btn_layanan"
                        android:layout_margin="30dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/btn_background"
                        android:fontFamily="@font/pp"
                        android:text="@string/ajukan"
                        android:textColor="@color/colorWhite"
                        android:textSize="18sp"
                        android:paddingLeft="20dp"
                        android:drawableLeft="@drawable/wa"
                        >



                    </Button>



                </RelativeLayout>
            </LinearLayout>



        </androidx.core.widget.NestedScrollView>
    </LinearLayout>


</RelativeLayout>


























